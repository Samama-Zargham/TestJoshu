{
  "name": "testProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:cache": "npx react-native start --reset-cache",
    "android": "npx react-native run-android",
    "install-apk": "react-native run-android --mode=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "yarn pod-install",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pod-install": "cd ios && pod install",
    "debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "jsbundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "rm-duplicate-drawables": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/*",
    "rm-duplicate": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "clean": "cd android && ./gradlew clean",
    "apk-debug": "cd android && ./gradlew assembleDebug",
    "apk-release": "cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "release": "cd android && ./gradlew :app:assembleRelease",
    "apk-release-app": "cd android && ./gradlew :app:assembleRelease -x bundleReleaseJsAndAssets",
    "bundle": "cd android && ./gradlew :app:bundleRelease",
    "cache:clean": "rm -rf node_modules package-lock.json ios/Pods ios/Podfile.lock && yarn"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.22.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.18",
    "add": "^2.0.6",
    "i18next": "^23.10.0",
    "react": "18.2.0",
    "react-i18next": "^14.0.5",
    "react-native": "0.73.4",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-maps": "^1.10.3",
    "react-native-maps-directions": "^1.9.0",
    "react-native-permissions": "^4.1.2",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-slider": "^0.11.0",
    "react-native-svg": "^15.1.0",
    "react-native-svg-transformer": "^1.3.0",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
